<?xml version="1.0"?>
<cd:ProductDefinition xmlns:cd="http://www.stcorp.nl/coda/definition/2008/07" xmlns:ct="http://www.stcorp.nl/coda/test/2008/10" id="BBR_NOM_0_" format="binary" last-modified="2020-02-25">
  <cd:Array>
    <cd:Dimension><![CDATA[$num_dsr]]></cd:Dimension>
    <cd:Record format="binary">
      <cd:Description>Annotated Instrument Source Packet</cd:Description>
      <cd:Field name="ISP_annotation_header">
        <cd:NamedType id="ISP_annotation_header"/>
      </cd:Field>
      <cd:Field name="ISP">
        <cd:Record format="binary">
          <cd:Description>Instrument Source Packet</cd:Description>
          <cd:Field name="packet_header">
            <cd:NamedType id="ISP_packet_header"/>
          </cd:Field>
          <cd:Field name="data_field_header">
            <cd:NamedType id="ISP_data_field_header"/>
          </cd:Field>
          <cd:Field name="data_field">
            <cd:Record format="binary">
              <cd:Field name="SC_state_vector_quality">
                <cd:Integer>
                  <cd:Description>S/C State Vector Quality</cd:Description>
                  <cd:BitSize>32</cd:BitSize>
                  <cd:NativeType>uint32</cd:NativeType>
                </cd:Integer>
              </cd:Field>
              <cd:Field name="ISP_format_version">
                <cd:Integer>
                  <cd:Description>ISP Format Version</cd:Description>
                  <cd:BitSize>16</cd:BitSize>
                  <cd:NativeType>uint16</cd:NativeType>
                </cd:Integer>
              </cd:Field>
              <cd:Field name="delimiter_0">
                <cd:Integer>
                  <cd:Description>Fixed Field Delimiter</cd:Description>
                  <cd:BitSize>16</cd:BitSize>
                  <cd:NativeType>uint16</cd:NativeType>
                </cd:Integer>
                <cd:Hidden/>
              </cd:Field>
              <cd:Field name="view_parameters">
                <cd:Array>
                  <cd:Description>View parameters for each acquisition</cd:Description>
                  <cd:Dimension>8</cd:Dimension>
                  <cd:Record format="binary">
                    <cd:Field name="aft_start_time">
                      <cd:Time timeformat="float(./coarse) + float(./fine) / 65536">
                        <cd:Record>
                          <cd:Description>Aft telescope ordered acquisition time</cd:Description>
                          <cd:Field name="coarse">
                            <cd:Integer>
                              <cd:BitSize>32</cd:BitSize>
                              <cd:NativeType>uint32</cd:NativeType>
                            </cd:Integer>
                          </cd:Field>
                          <cd:Field name="fine">
                            <cd:Integer>
                              <cd:BitSize>16</cd:BitSize>
                              <cd:NativeType>uint16</cd:NativeType>
                            </cd:Integer>
                          </cd:Field>
                        </cd:Record>
                      </cd:Time>
                    </cd:Field>
                    <cd:Field name="nadir_start_time">
                      <cd:Time timeformat="float(./coarse) + float(./fine) / 65536">
                        <cd:Record>
                          <cd:Description>Nadir telescope ordered acquisition start time</cd:Description>
                          <cd:Field name="coarse">
                            <cd:Integer>
                              <cd:BitSize>32</cd:BitSize>
                              <cd:NativeType>uint32</cd:NativeType>
                            </cd:Integer>
                          </cd:Field>
                          <cd:Field name="fine">
                            <cd:Integer>
                              <cd:BitSize>16</cd:BitSize>
                              <cd:NativeType>uint16</cd:NativeType>
                            </cd:Integer>
                          </cd:Field>
                        </cd:Record>
                      </cd:Time>
                    </cd:Field>
                    <cd:Field name="fore_start_time">
                      <cd:Time timeformat="float(./coarse) + float(./fine) / 65536">
                        <cd:Record>
                          <cd:Description>Fore telescope ordered acquisition start time</cd:Description>
                          <cd:Field name="coarse">
                            <cd:Integer>
                              <cd:BitSize>32</cd:BitSize>
                              <cd:NativeType>uint32</cd:NativeType>
                            </cd:Integer>
                          </cd:Field>
                          <cd:Field name="fine">
                            <cd:Integer>
                              <cd:BitSize>16</cd:BitSize>
                              <cd:NativeType>uint16</cd:NativeType>
                            </cd:Integer>
                          </cd:Field>
                        </cd:Record>
                      </cd:Time>
                    </cd:Field>
                    <cd:Field name="cal_drum_position">
